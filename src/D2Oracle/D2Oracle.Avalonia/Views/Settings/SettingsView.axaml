<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d2Oracle="using:D2Oracle.Avalonia"
             xmlns:viewModels="clr-namespace:D2Oracle.Core.ViewModels.Settings;assembly=D2Oracle.Core"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="D2Oracle.Avalonia.Views.Settings.SettingsView"
             x:DataType="viewModels:SettingsViewModel"
             DataContext="{d2Oracle:DISource Type=viewModels:SettingsViewModel}">
    <StackPanel Orientation="Vertical">
        <ContentControl Content="{Binding DotaConnectionSettingsViewModel}" />
    </StackPanel>
</UserControl>
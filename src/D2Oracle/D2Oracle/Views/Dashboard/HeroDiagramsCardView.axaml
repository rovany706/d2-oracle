<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="D2Oracle.Views.Dashboard.HeroDiagramsCardView"
             x:DataType="dashboard:HeroDiagramsCardViewModel"
             xmlns:lvc="using:LiveChartsCore.SkiaSharpView.Avalonia"
             xmlns:dashboard="clr-namespace:D2Oracle.Core.ViewModels.Dashboard;assembly=D2Oracle.Core">
    <Border Classes="Card">
        <lvc:CartesianChart Series="{Binding NetWorthSeries}" XAxes="{Binding NetWorthXAxes}" YAxes="{Binding NetWorthYAxes}" />
    </Border>
</UserControl>